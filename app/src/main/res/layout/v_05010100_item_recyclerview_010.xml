                        android:fontFamily="@font/monoespaciada_principal"
                        android:text="0.00"
                        android:textColor="@color/black"
                        android:textSize="15.5dp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toEndOf="@+id/textView39"
                        app:layout_constraintTop_toTopOf="@+id/textView39" />

                    <TextView
                        android:id="@+id/textView40"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="4dp"
                        android:fontFamily="@font/monoespaciada_principal"
                        android:text="Rdtos:"
                        android:textSize="15.5dp"
                        app:layout_constraintEnd_toStartOf="@+id/c010_txv_Rdtos_v"
                        app:layout_constraintTop_toTopOf="@+id/c010_txv_Horas_v" />

                    <TextView
                        android:id="@+id/c010_txv_Rdtos_v"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="5dp"
                        android:fontFamily="@font/monoespaciada_principal"
                        android:text="0.00"
                        android:textColor="@color/black"
                        android:textSize="15.5dp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/textView40" />

                    <TextView
                        android:id="@+id/textView63"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:fontFamily="@font/monoespaciada_principal"
                        android:text="Ingreso:"
                        android:textSize="15.5dp"
                        app:layout_constraintStart_toStartOf="@+id/textView39"
                        app:layout_constraintTop_toBottomOf="@+id/textView39" />

                    <TextView
                        android:id="@+id/tvIngreso"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="2dp"
                        android:fontFamily="@font/monoespaciada_principal"
                        android:text="YYYY-MM-DD HH:SS:mm"
                        android:textSize="15.5dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/textView63"
                        app:layout_constraintTop_toTopOf="@+id/textView63" />

                    <TextView
                        android:id="@+id/textView71"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:fontFamily="@font/monoespaciada_principal"
                        android:text="Salida:"
                        android:textSize="15.5dp"
                        app:layout_constraintStart_toStartOf="@+id/textView63"
                        app:layout_constraintTop_toBottomOf="@+id/textView63" />

