                        android:fontFamily="@font/vmr"
                        android:text="Desc Cultivo"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/c010_txv_IdCultivo_v"
                        app:layout_constraintTop_toTopOf="@+id/c010_txv_IdCultivo_v" />

                    <TextView
                        android:id="@+id/textView68"
                        android:layout_width="0.2px"
                        android:layout_height="0.2px"
                        android:fontFamily="@font/vmr"
                        android:text="Variedad:"
                        android:textSize="14sp"
                        app:layout_constraintStart_toStartOf="@+id/textView65"
                        app:layout_constraintTop_toBottomOf="@+id/textView65" />

                    <TextView
                        android:id="@+id/c010_txv_IdVariedad_v"
                        android:layout_width="0.2px"
                        android:layout_height="0.2px"
                        android:layout_marginStart="2dp"
                        android:fontFamily="@font/vmr"
                        android:text="(001)"
                        android:textColor="@color/black"
                        android:textSize="14sp"
                        android:textStyle="bold"
                        app:layout_constraintStart_toEndOf="@+id/textView68"
                        app:layout_constraintTop_toTopOf="@+id/textView68" />

                    <TextView
                        android:id="@+id/c010_txv_Variedad_v"
                        android:layout_width="0.2px"
