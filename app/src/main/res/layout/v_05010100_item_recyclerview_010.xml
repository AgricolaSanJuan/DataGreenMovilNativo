                    <TextView
                        android:id="@+id/c010_txv_Labor_v"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="2dp"
                        android:ellipsize="end"
                        android:fontFamily="@font/monoespaciada_principal"
